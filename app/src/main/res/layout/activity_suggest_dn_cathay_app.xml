<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#9de1f2"
    tools:context=".suggest_dn_cathay_app">

    <View
        android:id="@+id/myRectangleView"
        android:layout_width="300dp"
        android:layout_height="400dp"
        android:layout_marginStart="56dp"
        android:layout_marginEnd="56dp"
        android:layout_marginTop="285dp"
        android:layout_marginBottom="285dp"
        android:background="@drawable/my_rectangle_def8ff"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/叉叉_image"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@android:drawable/ic_menu_close_clear_cancel"
        android:background="@android:color/transparent"
        android:layout_marginEnd="68dp"
        android:layout_marginBottom="455dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />
    <!--519dp-->
    <TextView
        android:id="@+id/種樹賺點_text"
        android:layout_width="350dp"
        android:layout_height="100dp"
        android:gravity="center"
        android:text="種種樹苗，賺小樹點"
        android:textColor="#54dcff"
        android:textSize="26dp"
        android:textStyle="bold"
        android:layout_marginStart="56dp"
        android:layout_marginEnd="56dp"
        android:layout_marginBottom="366dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/國泰優惠_icon_image"
        android:layout_width="100dp"
        android:layout_height="100dp"
        app:srcCompat="@drawable/myrewards"
        android:layout_marginStart="56dp"
        android:layout_marginEnd="56dp"
        android:layout_marginBottom="273dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/得到的小樹點_text"
        android:layout_width="210dp"
        android:layout_height="95dp"
        android:gravity="center_vertical|left"
        android:text="快來「國泰優惠」\n查看你的小樹點！"
        android:textColor="#54dcff"
        android:textSize="22dp"
        android:layout_marginStart="70dp"
        android:layout_marginEnd="120dp"
        android:layout_marginBottom="170dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="45dp"
        android:layout_height="95dp"
        app:srcCompat="@drawable/logo_treepoint"
        tools:ignore="MissingConstraints"
        android:layout_marginStart="271dp"
        android:layout_marginEnd="70dp"
        android:layout_marginBottom="170dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/去GooglePlay下載_image"
        android:layout_width="150dp"
        android:layout_height="90dp"
        app:srcCompat="@drawable/google_play_badge"
        android:layout_marginStart="200dp"
        android:layout_marginEnd="200dp"
        android:layout_marginBottom="101dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>