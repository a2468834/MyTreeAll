<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#def8ff"
    tools:context=".my_forest">

    <TextView
        android:id="@+id/建立小樹苗_text"
        android:layout_width="95dp"
        android:layout_height="30dp"
        android:gravity="center"
        android:text="建立小樹苗"
        android:textColor="@color/black"
        android:textSize="15dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.284"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.566" />

    <TextView
        android:id="@+id/理財試算_text"
        android:layout_width="95dp"
        android:layout_height="30dp"
        android:gravity="center"
        android:text="理財試算"
        android:textColor="@color/black"
        android:textSize="15dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.712"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.566" />

    <TextView
        android:id="@+id/PS5_text"
        android:layout_width="50dp"
        android:layout_height="25dp"
        android:layout_marginBottom="0dp"
        android:gravity="center"
        android:text="PS5"
        android:textColor="@color/black"
        android:textSize="14dp"
        app:layout_constraintBottom_toTopOf="@+id/level8_tree_image"
        app:layout_constraintEnd_toEndOf="@+id/level8_tree_image"
        app:layout_constraintStart_toStartOf="@+id/level8_tree_image" />

    <TextView
        android:id="@+id/特斯拉_text"
        android:layout_width="50dp"
        android:layout_height="25dp"
        android:layout_marginBottom="0dp"
        android:gravity="center"
        android:text="特斯拉"
        android:textColor="@color/black"
        android:textSize="13dp"
        app:layout_constraintBottom_toTopOf="@+id/level5_tree_image"
        app:layout_constraintEnd_toEndOf="@+id/level5_tree_image"
        app:layout_constraintStart_toStartOf="@+id/level5_tree_image" />

    <TextView
        android:id="@+id/第一桶金_text"
        android:layout_width="60dp"
        android:layout_height="25dp"
        android:layout_marginBottom="0dp"
        android:gravity="center"
        android:text="第一桶金"
        android:textColor="@color/black"
        android:textSize="13dp"
        app:layout_constraintBottom_toTopOf="@+id/level2_tree_image"
        app:layout_constraintEnd_toEndOf="@+id/level2_tree_image"
        app:layout_constraintStart_toStartOf="@+id/level2_tree_image" />

    <TextView
        android:id="@+id/管理室_text"
        android:layout_width="60dp"
        android:layout_height="25dp"
        android:layout_marginBottom="0dp"
        android:gravity="center"
        android:text="國泰優惠"
        android:textColor="@color/black"
        android:textSize="13dp"
        app:layout_constraintBottom_toTopOf="@+id/管理室_image"
        app:layout_constraintEnd_toEndOf="@+id/管理室_image"
        app:layout_constraintStart_toStartOf="@+id/管理室_image" />

    <ImageView
        android:id="@+id/建立小樹苗_image"
        android:layout_width="77dp"
        android:layout_height="77dp"
        android:gravity="center"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/建立小樹苗_text"
        app:layout_constraintHorizontal_bias="0.521"
        app:layout_constraintStart_toStartOf="@+id/建立小樹苗_text"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.478"
        app:srcCompat="@drawable/ecology" />

    <ImageView
        android:id="@+id/理財試算_image"
        android:layout_width="77dp"
        android:layout_height="77dp"
        android:gravity="center"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/理財試算_text"
        app:layout_constraintHorizontal_bias="0.521"
        app:layout_constraintStart_toStartOf="@+id/理財試算_text"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.478"
        app:srcCompat="@drawable/farmer" />

    <ImageView
        android:id="@+id/雲朵_medium_image"
        android:layout_width="75dp"
        android:layout_height="75dp"
        android:gravity="center"
        app:srcCompat="@drawable/cloud_computing"
        tools:ignore="MissingConstraints"
        android:layout_marginEnd="255dp"
        android:layout_marginBottom="415dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <ImageView
        android:id="@+id/雲朵_large_image"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:gravity="center"
        app:srcCompat="@drawable/cloud_computing"
        tools:ignore="MissingConstraints"
        android:layout_marginEnd="95dp"
        android:layout_marginBottom="443dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <ImageView
        android:id="@+id/雲朵_small_image"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:gravity="center"
        app:srcCompat="@drawable/cloud_computing"
        tools:ignore="MissingConstraints"
        android:layout_marginEnd="120dp"
        android:layout_marginBottom="368dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <ImageView
        android:id="@+id/太陽_image"
        android:layout_width="90dp"
        android:layout_height="90dp"
        app:srcCompat="@drawable/sun"
        tools:ignore="MissingConstraints"
        android:layout_marginEnd="19dp"
        android:layout_marginBottom="492dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />
<!--
    <ImageView
        android:id="@+id/返回鍵_image"
        android:layout_width="40dp"
        android:layout_height="40dp"
        app:srcCompat="?attr/actionModeCloseDrawable"
        android:background="@android:color/transparent"
        android:scaleType="fitCenter"
        tools:ignore="MissingConstraints"
        android:layout_marginEnd="351dp"
        android:layout_marginBottom="542dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />
-->
    <ImageView
        android:id="@+id/土地_image"
        android:layout_width="match_parent"
        android:layout_height="200dp"
        android:gravity="center"
        android:scaleType="fitEnd"
        app:layout_constraintBottom_toBottomOf="parent"
        app:srcCompat="@drawable/ground"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/管理室_image"
        android:layout_width="65dp"
        android:layout_height="65dp"
        android:layout_marginEnd="35dp"
        android:layout_marginBottom="138dp"
        android:gravity="center"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/barn"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/level2_tree_image"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginEnd="328dp"
        android:layout_marginBottom="139dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/level2"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/level5_tree_image"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginEnd="240dp"
        android:layout_marginBottom="144dp"
        android:gravity="center"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/level5"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/level8_tree_image"
        android:layout_width="80dp"
        android:layout_height="70dp"
        android:layout_marginEnd="135dp"
        android:layout_marginBottom="140dp"
        android:gravity="center"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/level8"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/小樹苗森林_text"
        android:layout_width="230dp"
        android:layout_height="55dp"
        android:gravity="center"
        android:text="小樹苗森林"
        android:textColor="@color/black"
        android:textSize="40dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.249" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_left"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_begin="20dp" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_right"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_end="20dp" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_top"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_begin="20dp" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_bottom"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_end="20dp" />

</androidx.constraintlayout.widget.ConstraintLayout>